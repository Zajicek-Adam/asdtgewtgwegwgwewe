@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    Layout = "_Layout-Index";
}
<article id="first" class="full grid">
    <div class="hidden fh" id="heading">
        <h1>
            01
        </h1>
        <h2>
            Collect&amp;Share
        </h2>
        <h3>
            All your favourite images<br />arranged in albums
        </h3>
    </div>
    <div class="hidden fh" id="bg">
        <partial name="_LoginPartial" />
        <img id="fancyhorse" height="520" src="~/img/horse.png" />
    </div>
</article>
<article class="full">
    <div style="flex-direction: column">
        <h2 style="margin-top: 2em;" class="tl-h hidden">Be up to date with meowmeows</h2>
